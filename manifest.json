{
  "manifest_version": 2,
  "name": "TubeTest",
  "description": "Test your knowledge after watching Youtube Videos",
  "version": "0.0.0.1",
  "browser_action": {
    "default_icon": "images/red.png"
  },
  "background": {
    "scripts": ["bg.js"]
  },

  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage",
    "activeTab",
    "tabs"
  ]
}